<% provide(:title, @user.name) %>
<div class="row">
<aside class="col-md-4">
<section class="user_info">
<h1>
<%= gravatar_for @user %>
<%= @user.name %>
</h1>
</section>
</aside>
</div>
<h3>User Profile Page</h3>
<p>Name:    <%= @user.name%>       </p>
<p>Email:   <%= @user.email %>     </p>
<p>Created: <%= @user.created_at %></p>
<p>Updated: <%= @user.updated_at %></p>
<p>Time:    <%= Time.now %>        </p>
